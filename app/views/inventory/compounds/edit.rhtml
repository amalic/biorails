<% content_for :sidemenu do%>
  <%=   render :partial =>'menu/default_left_menu' %>    
   <div>
    <span class='menu'>Compound </span>
    <%= link_to subject_icon('show.png')  + 'Show',   :action => 'show' %>
    <%= link_to subject_icon('list.png') + 'List',  :action => 'list' %>
  </div>
<% end %>

<div class='panel'>
    <div class="dialog">                              
        <% form_tag :action => 'update', :id => @compound do %>
          <%= render :partial => 'form' %>
          <%= submit_tag 'Save' %>
        <% end %>
    </div>
</div>
   
