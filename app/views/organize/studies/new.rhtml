<% content_for :messages do -%>
<%= render :partial => 'shared/messages', :locals => { :objects => ['study']}  %>
<%end %>

<% content_for :centre do -%>

<h2><%=subject_icon('study.png')%> New Study </h2>
    <% form_tag :action => 'create' do %>
    <%= render :partial => 'form' %>
    <div class="row">
        <%= submit_tag "Save" %>
        <%= link_to 'Cancel', :action => 'list' %>
    </div>
    <% end %>
<% end %>

<% content_for :right do -%>
    <%= render :partial => 'help' %>
<% end %>
