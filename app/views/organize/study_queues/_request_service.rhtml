           <%= observe_field  request_service.dom_id('user_id'),
                             :url=>{:action=>'update_service',:id=>request_service},
                             :with =>'user_id' %> 
          </td>
         <td><%=request_service.updated_by%> </td>
         <td><%=short_date request_service.updated_at%> </td>
