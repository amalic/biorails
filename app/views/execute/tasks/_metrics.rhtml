<table class="report">
  <tr>
    <th colspan='13'>
  </tr>
  <tr>
    <th colspan='3'><b class='icon icon-parameter'><%=l(:label_parameter)%></b></th>
    <th colspan='5'><b class='icon icon-task-small'><%=l(:label_task)%> (<%=l(:label_current)%>)</b></th>
    <th colspan='5'><b class='icon icon-protocol_version'><%=l(:label_process_version)%> (<%=l(:label_historic)%>)</b></th>
  </tr>
  <tr>
    <th rowspan='1' scope="col">
      <%=l(:label_alias)%>
    </th>
    <th rowspan='1' scope="col"><b class='icon icon-parameter_type'><%=l(:label_type)%></b></th>
    <th rowspan='1' scope="col"><b class='icon icon-parameter_role'><%=l(:label_role)%></b></th>

    <th rowspan='1' scope="col"><%=l(:stat_total)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_mean)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_sd)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_min)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_max)%></th>

    <th rowspan='1' scope="col"><%=l(:stat_total)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_mean)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_sd)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_min)%></th>
    <th rowspan='1' scope="col"><%=l(:stat_max)%></th>
  </tr>
<%for row in @task.statistics%>
  <tr class="<%= cycle('odd', 'even') %>">
    <td>
      <p class='icon icon-data_type_<%=row.data_type.name%>'>
        <%= row.data_type.name if row.data_type  %>
        <%=row.parameter.name if row.parameter %>
      </p>
    </td>
    <td> <%=row.type.name if row.type %>   </td>
    <td>
      <p class='icon icon-<%=row.role.name%>'>
          <%=row.role.name if row.role %>
      </p>
    </td>
    <td> <%=row.num_values %>  </td>
    <td> <%=number_round row.avg_values %>   </td>
    <td> <%=number_round row.stddev_values %>   </td>
    <td> <%=row.min_values %>   </td>
    <td> <%=row.max_values %>   </td>
    <td> <%=row.num_process %>  </td>
    <td> <%=number_round row.avg_process %>   </td>
    <td> <%=number_round row.stddev_process %> </td>
    <td> <%=row.min_process  %>  </td>
    <td> <%=row.max_process  %>  </td>
  </tr>
<%end%>
  <tr>
    <th colspan="13" scope="col"></th>
  </tr>
</table>
