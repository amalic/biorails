<% content_for :messages do -%>
  <%=render :partial =>'shared/messages',:locals => { :objects => ['experiment','project_folder' ]} %>
<%end%>

<% content_for :center do -%>
  <%= render :partial=>'header'%>
  <%= render :partial => '/folders/blog',:locals=>{:folder=>@project_folder} %>
<% end %>

<% content_for :right do -%>
  <div class='panel' id='folder_actions'>
    <%= render :partial => '/folders/selector',:locals=>{:folder=> @project_folder} %>   
  </div>
<% end %>