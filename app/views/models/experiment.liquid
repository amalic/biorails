<h2> Experiment {{record.name}} </h2>
<p>
{{record.description}}
</p>
<table WIDTH="99%" BORDER="1" CELLPADDING="1" CELLSPACING="0">
<tr>
<th BGCOLOR='#cccccc'>Task</th>
<th BGCOLOR='#cccccc'>Assigned</th>
<th BGCOLOR='#cccccc'>Status</th>
<th BGCOLOR='#cccccc'>Started</th>
<th BGCOLOR='#cccccc'>Expected</th>
</tr>
{% for item in experiment.tasks %}
<tr>
<th BGCOLOR='#cccccc'>{{ item.name}}</th> 
<td>{{item.assigned_user }}</th>
<td>{{item.status }} </th>
<td>{{item.started }}</th>
<td>{{item.expected }}</th>
</tr>
{% endfor %}
</table>
