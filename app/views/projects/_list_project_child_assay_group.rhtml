<div class='box'>
  <strong class='icon icon-project-assaygroup'><%=l(:label_assay_groups)%></strong>
    <dl>
    <% for item in @project.children_of_type(8,2) %>
      <dt><%= link_to item.name, project_url( :action => 'show', :id => item) %>
        </dt>
      <dd><%= item.description %>
        </dd>
     <% end %>
    </dl>
  <hr/>
  <%= link_to_if allows?(:data,:create), l(:label_assay_group), project_url(:action => 'new', :type=>2), :class=>'icon icon-new'%>
</div>
