<%= hidden_field :parameter_role, :lock_version  %>

<p>
  <label for="parameter_role_name" >
    <%=l(:label_name)%>
  </label>
  <br/>
  <%= text_field 'parameter_role', 'name',:size=>'80'  %>
</p>

<p>
  <label for="parameter_role_description" >
    <%=l(:label_description)%>
  </label>
  <br/>
  <%= text_area( 'parameter_role', 'description',:rows=>4,:cols=>80)  %>
</p>

<p>
  <label for="parameter_role_weighing">
    <%=l(:label_weighting)%>
  </label>
  <br/>
  <%= text_field 'parameter_role', 'weighing'  %>
</p>

