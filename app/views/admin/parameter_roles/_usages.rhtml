<table class="report">
  <tr>
    <th>
      <%=l(:Assay)%><%=l(:Name)%>
    </th>
    <th>
      <%=l(:Assay)%><%=l(:Parameter)%>
    </th>
    <th><%=l(:Parameter_Type)%></th>
    <th><%=l(:Data_Type)%></th>
    <th><%=l(:Data_Format)%></th>
    <th><%=l(:Data_Element)%></th>
  </tr>
  <%for item in  @parameter_role.assay_parameters %>
    <tr class="<%= cycle('odd', 'even') %>">
      <td>
        <%=link_to item.assay.name, assay_url(:action=>:show , :id=>item.assay) %>
      </td>
      <td>
        <%=link_to item.name, assay_parameter_url(:action=>:show ,:id=>item.id) %>
      </td>
      <td>
        <%=link_to item.parameter_type.name, parameter_type_url(:action=>:show  ,:id=>item.parameter_type.id) if item.parameter_type%>
      </td>
      <td>
        <p class='icon icon-data_type_<%= h item.data_type.name%>'>
          <%=item.data_type.name%>
        </p>
      </td>
      <td><%=h item.data_format.name if item.data_format_id %></td>
      <td><%=h item.data_element.name if item.data_element_id %></td>
    </tr>
  <%end%>
</table>
