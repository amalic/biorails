<% content_for :sidemenu do%>
  <%= render :partial => 'menu'%>
<% end %>

<% content_for :messages do -%>
  <%=render :partial =>'shared/messages',:locals => { :objects => ['parameter_role']} %>
<%end%>

<% content_for :center do%>
<%= render :partial => 'admin/header' %>
  <% dialog l(:Parameter_Role_Edit) do %>     
    <%=l(:text_parameter_role_new_full)%>   
    <% validated_form_tag :action => 'update', :id => @parameter_role do %>
      <%= render :partial => 'form' %>
      <%= submit_tag l(:Save) %>
      <%= link_to l(:Cancel),  parameter_role_url(:action => 'list') %>
    <% end %>
  <% end %>
<% end %>


<% content_for :right do -%>
	<%= render :partial=>'help' %>
<%end%>