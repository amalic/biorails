<%= render :partial => '/menu/admin_menu'%>
<div>
<span class='menu'>Data Type</span>
    <p class="icon icon-list"><%= link_to 'List',    data_type_url(:action => 'list') %></p>
    <p class="icon icon-new"><%= link_to 'New',     data_type_url(:action => 'new') %> </p>
    <% if @data_type && !@data_type.new_record? %>
	    <p class="icon icon-edit"><%= link_to 'Edit',    data_type_url(:action => 'edit', :id => @data_type) %> </p>
	    <p class="icon icon-show"><%= link_to 'Show',    data_type_url(:action => 'show',:id => @data_type) %></p>
    <% end %>
</div>