<% content_for :sidemenu do%>
  <%= render :partial => 'menu'%>
<% end %>
<% content_for :right do%>
  <%= render :partial => 'help'%>
<% end %>

<% content_for :messages do -%>
  <%=render :partial =>'shared/messages',:locals => { :objects => ['data_type']} %>
<%end%>

<% dialog '<h1 class="icon icon-data_type">Data Type</h1>' do %>
	<% form_tag :action => 'update', :id => @data_type do %>
	  <%= render :partial => 'form' %>
	  <%= submit_tag 'Edit' %>
	<% end %>
<% end %>
