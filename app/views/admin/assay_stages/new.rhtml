<% content_for :sidemenu do%>
  <%= render :partial => '/menu/admin_menu'%>
  <div>
    <span class='menu'>
      <b class='icon icon-assay_stage'>
        <%=l(:label_assay_stage)%>
      </b>
    </span>
      <%= link_to l(:label_list), :action => 'list' %>
  </div>
<% end %>
<%= render :partial => 'admin/header'%>
<% dialog l(:label_assay_stage) do %>
	<% form_tag :action => 'create' do %>
	  <%= render :partial => 'form' %>
	  <%= submit_tag l(:button_save) %>
          <%= link_to l(:label_cancel), :action => 'list'%>
	<% end %>
<% end %>
