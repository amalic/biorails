<% dialog l(:Data_Element) do %>
<p>
    <%=l(:text_concept_implementation) %>
    [<%=@data_concept.path %>] <%=l(:text_below) %>
</p>
<%= form_remote_tag :url => {:action =>'create_element', :id => @data_concept} %>
<%= render :partial =>'form_element' %>
<div class="row">
    <%= submit_tag l(:Save) %>
    <%=link_to_remote l(:Cancel), :url =>{:action=>'show',:id => @data_concept.id} %>
</div>
<% end %>