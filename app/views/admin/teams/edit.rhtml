<% content_for :sidemenu do -%>
  <%= render :partial =>'/menu/team_menu', :locals=>{ :team => @team}  %>
<% end -%>

<% content_for :messages do -%>
  <%=render :partial =>'shared/messages',:locals => { :objects => ['team','user']} %>
<% end -%>

<% content_for :center do -%>

    <% form_tag :action => 'update', :id => @team.id do %>
    <%= render :partial => 'form' %>
    <div class="row">
        <%= submit_tag "Save" %>
    </div>
    <% end %>
<% end %>
