<%= render :partial => '/menu/admin_menu' %>
<div>
    <span class='menu'><b class='icon icon-data_system'><%=l(:label_data_source) %>
        </b></span>
    <%= link_to l(:label_list), :action => 'list' %>
    <%= link_to l(:label_new), :action => 'new' %>
    <%if @data_system && !@data_system.new_record? %>
    <%= link_to l(:label_show), :action => 'show', :id =>  @data_system %>
    <%= link_to l(:label_edit), :action => 'edit', :id =>  @data_system %>
    <% end %>
</div>
