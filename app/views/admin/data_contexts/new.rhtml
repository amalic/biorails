<% content_for :sidemenu do%>
  <%= render :partial => '/menu/admin_menu'%>
    <div>
    <span class='menu'>
      <%=l(:New)%>
      </span>
      <li>
        <%= link_to List, :action => 'list' %>
      </li>
    </div>
<% end %>
<%= render :partial => 'admin/header' %>
<div class='main' id='maintitle'>
    <h1 class='icon icon-data_context'>
        <%=l(:Dictionary_New)%>
    </h1>
    <div class='dialog'>
        <% form_tag :action => 'create' do %>
          <%= render :partial => 'form' %>
          <%= submit_tag "Create" %>
       <% end %>
    </div>
</div>
