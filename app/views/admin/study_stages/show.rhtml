<% content_for :sidemenu do%>
<%= render :partial => '/menu/admin_menu'%>
<div>
<span class='menu'>StudyStage</span>
<%= link_to 'List',    study_stage_url(:action => 'list') %>
<%= link_to 'New',    study_stage_url(:action => 'new') %> 
<%= link_to 'Edit',    study_stage_url(:action => 'edit', :id => @study_stage) %> 
