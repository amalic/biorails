<% content_for :center do -%>
  <h1 class='icon icon-team-large' style='text-indent:0.5em'>
    Team member [<strong><%=@current_user.name%></strong>] in <%=@team.name%>
  </h1>
  <% for column in Membership.content_columns %>
  <p>
    <b><%= column.human_name %>:</b> <%=h @membership.send(column.name) %>
  </p>
  <% end %>
<%end%>

<% content_for :sidemenu do -%>
  <%= render :partial=>'actions'%>
<% end -%>
