<div class='main' id='maintitle'>
    <div class="title">
        <%=subject_icon('study.png')%> New Request
        <div class="actions">
            <ul>
              <li><%= link_to (subject_icon('list.png')) + 'List', :action => 'list' %></li>
            </ul>
        </div>
    </div>

    <%= error_messages_for 'request' %>

    <div class="dialog">                              
      <% form_tag :action => 'create' do %>
        <%= render :partial => 'form' %>
        <div class="row">
            <%= submit_tag "Save" %>
        </div>
      <% end %>
    </div>
</div>
