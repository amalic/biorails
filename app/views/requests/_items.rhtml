<table class='report' style='border: solid thin'>
    <tr>
         <th>name</th>         
         <%for item in request.services %>
         <th><%=link_to item.name+": "+item.queue.name , :controller => 'request_services', :action => 'show', :id => item %> </th>
         <% end %>
         <th>action</th>                   
    </tr>
    <% grid = request.items_by_service %>
    <%for item in request.items %>
    <tr>
         <td><%=item.data_name%> </td>
         <%for service in request.services %>
             <% if  grid[item.data_name][service.name] %>
             <td class='state-<%=grid[item.data_name][service.name].status%>' ><%=   grid[item.data_name][service.name].status    %> </td>
             <% else %>
             <td><not submitted </td>
             <% end %>
         <% end %>
         <td><%= link_to 'Remove', :action => 'remove_item',:id=> request, :list_item_id => item.id %> </td>
    </tr>
     <% end %>
</table>