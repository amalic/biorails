<center>
<div class="box login">
<%= error_messages_for 'user' %>

<% dialog subject_icon('logo/logo_full.png') do %>
  <%= flash[:notice]%>
    <% form_tag :controller=>:auth, :action=>:login do %>
    <div class="row">
      <!--[form:assay]-->
      <div class="form-element">
         <label for="assay_name"><%=l(:label_login)%></label>
        <%= text_field_tag 'login', params[:login], :class => 'big' %>
      </div>
     </div>

    <div class="row">
      <div class="form-element">
       <label for="assay_description">
         <%=l(:label_password)%>
       </label>
        <%= password_field_tag 'password', {}, :class => 'big' %>
      </div>
    </div>
  
    <div class="row">
        <%= submit_tag l(:label_login) %>
    </div>
    <% end %>
  <% end %>
  </div>
  </center>
<script type="text/javascript">
javascript:window.history.forward(1);
</script>