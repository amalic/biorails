<h2 class='icon icon-sign'>
    <%=l(:label_signed) %>
    <%=l(:label_documents) %>
</h2>
<div class='panel'>
        <div class="box">
          <%= @title %>
        </div>
        <%= render :partial => 'signatures/report'%>
        <hr/>
        <div class='pagination'>
            <%= link_to "Signed", :action=>'list' %>
            <%= link_to "Published", :action=>'published' %>
            <%= link_to "Authored", :action=>'list_authored' %>
            <%= link_to "Witnessed", :action=>'list_witnessed' %>
            <%= link_to "To Witness", :action=>'list_pending' %>
            <%= link_to "In Review", :action=>'list_waiting' %>
        </div>
    
</div>
